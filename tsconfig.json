{
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": false,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "paths": {
      "*": ["node_modules/*"],
    "lib": [
      "es2021"
    ]
    }
  },
  "include": ["src/**/*", "electron/**/*"]
}
